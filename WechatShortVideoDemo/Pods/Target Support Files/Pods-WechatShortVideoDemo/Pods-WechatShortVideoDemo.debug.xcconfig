OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/PureLayout" -isystem "${PODS_ROOT}/Headers/Public/SCRecorder"
OTHER_LDFLAGS = $(inherited) -ObjC -l"MBProgressHUD" -l"PureLayout" -l"SCRecorder" -framework "AVFoundation" -framework "CoreGraphics" -weak_framework "GLKit" -weak_framework "Metal"
PODS_ROOT = ${SRCROOT}/Pods